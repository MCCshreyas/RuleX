<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Shreyas Jejurkar</Authors>
    <Company />
    <Description>Simple, clean, and object-oriented way to create and execute business rules in .NET</Description>
    <Copyright>copyright Shreyas Jejurkar</Copyright>
    <PackageProjectUrl>https://github.com/MCCshreyas/RuleX</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MCCshreyas/RuleX</RepositoryUrl>
    <RepositoryType>public</RepositoryType>
    <PackageTags>rules rule-engine rule-design-pattern csharp dotnet business-rules business-rules-engine</PackageTags>
    <PackageReleaseNotes>First Patch release</PackageReleaseNotes>
    <SignAssembly>false</SignAssembly>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath></OutputPath>
  </PropertyGroup>

</Project>
